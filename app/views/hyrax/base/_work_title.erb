<% presenter.title.each_with_index do |title, index| %>
    <% if index == 0 %>
        <h1><%= title %> <%= presenter.permission_badge %></h1>
    <% else %>
        <h1><%= title %></h1>
    <% end %>
<% end %>
<% presenter.creator.each_with_index do |creator, index| %>
  <h2>
    <%= creator %> (<%= presenter.graduation_date %>)
  </h2>
<% end %>
