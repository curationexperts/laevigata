<% if registrar_feed.errors.any? %>
  <div id="error_explanation" class="alert alert-danger alert-dismissable" role="alert">
    <h2><%= pluralize(registrar_feed.errors.count, "error") %> prohibited this registrar_feed from being saved:</h2>

    <ul>
      <% registrar_feed.errors.full_messages.each do |message| %>
        <li><%= message %></li>
